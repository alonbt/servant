{
  "manifest_version": 2,

  "name": "Servant - Server Mock",
  "description": "This extension will help you mock client requests to the server",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}